{
  "AllowedHosts": "*",
  "EnableConsoleMetrics": false,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Information",
        "Microsoft.Hosting": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  }
}